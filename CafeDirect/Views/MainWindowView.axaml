<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="CafeDirect.Views.MainWindowView"
        xmlns:reactiveUi="http://reactiveui.net"
        xmlns:ViewModels="clr-namespace:CafeDirect.ViewModels"
        x:DataType="ViewModels:MainWindowViewModel"
        Title="Cafe Direct">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <reactiveUi:RoutedViewHost Grid.Row="0" Router="{Binding Router}">
            <reactiveUi:RoutedViewHost.DefaultContent>
                <TextBlock Text="Default Content" />
            </reactiveUi:RoutedViewHost.DefaultContent>
        </reactiveUi:RoutedViewHost>
        <Grid>
            <Button Grid.Row="1" Content="AUTH" Command="{Binding AuthWindow}"></Button>
        </Grid>
    </Grid>
</Window>